let $filaCabecera = $("<tr>")
        .append($("<th/>",{
            html: "<input type='checkbox' name='borrartodos' id='borrartodos'/>"
        })).append($("<th/>",{
            text: "Nombre"
        })).append($("<th/>",{
            text: "Apellidos"
        })).append($("<th/>",{
            text: "DNI"
        })).append($("<th/>",{
            text: "Email"
        })).append($("<th/>"));

let $tabla = $("<table/>",{
        "data-table" : "alumnos",
        id: "tablaAlumnos",
        class: "rwd-table"
}).append(
    $("<thead/>")
        .append($filaCabecera)
);

$("#contenedor").append($tabla);


